  "version": "1.0.0",
  "description": "CS 341 @ Illinois Autograder Web App",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "setup": "git config blame.ignoreRevsFile .git-blame-ignore-revs || exit 0",
    "build": "npm run prisma:generate && npm run build:server && vite build",
    "build:server": "tsc",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "dev": "tsx src/server.ts --dev | pino-pretty",
    "start:prod": "node dist/server.js",
    "prisma:generate": "prisma generate && cp src/generated/prisma/enums.ts src/client/",
    "prisma:studio": "prisma studio --port 9090",
    "prisma:migrate-dev": "prisma migrate dev && cp src/generated/prisma/enums.ts src/client/"
  },
